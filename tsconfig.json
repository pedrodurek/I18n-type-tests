{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noEmit": true,
    "baseUrl": "./src",
    "paths": {
      "components": ["components"]
    }
  }
}
